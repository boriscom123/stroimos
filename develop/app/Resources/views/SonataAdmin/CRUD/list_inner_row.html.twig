{% for field_description in admin.list.elements %}
    {% if field_description.name == '_action' and (app.request.isXmlHttpRequest or ckParameters is defined) %}
        {# Action buttons disabled in ajax view and browser #}
    {% elseif field_description.getOption('code') == '_batch' and ckParameters is defined %}
        {# Disable batch in browser #}
    {% elseif field_description.getOption('ajax_hidden') == true and app.request.isXmlHttpRequest %}
        {# Disable fields with 'ajax_hidden' option set to true #}
    {% else %}
        {{ object|render_list_element(field_description) }}
    {% endif %}
{% endfor %}

